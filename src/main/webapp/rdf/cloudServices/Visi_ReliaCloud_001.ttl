:slp_ReliaCloud a usdl:ServiceLevelProfile ;
	dcterms:title "ReliaCloud SLA"@en ;
	dcterms:description "ReliaCloud SLA v1.1 (Feb 8th 2010), c.f. 'http://www.reliacloud.com/legal/sla/ '. The remedies in the SLA are your sole and exclusive remedies for any Incident or other failure of the Service."@en ;
	sla:hasServiceLevel 
		:slo_CloudServer_Availability ,
		:slo_CloudServer_Availability_Exclusions ,
		:slo_CloudServer_UnavailabilityReport ,
		:slo_CloudServer_Credit_Calculation ,
		:slo_CloudServer_Credit_Suspension ,
		:slo_CloudServer_Credit_Request .

#____________________________________________________________________________________
:slo_CloudServer_Availability a sla:GuaranteedState ;
	dcterms:title "Availability"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "ReliaCloud Cloud Server guarantees a 100% availability of network, instrastructure and host servers."@en ;
			sla:hasVariable 
				:var_CloudServer_Availability_Value , 
				:var_CloudServer_Availability_MeasurementPeriod ,
				:var_CloudServer_Availability_Network ,
				:var_CloudServer_Availability_Infrastructure , 
				:var_CloudServer_Availability_HostServer ] .

:var_CloudServer_Availability_Value a sla:Variable ;
	rdfs:label "100% Availability"@en ;
	sla:hasDefault 
		[ a cloud:Availability ;
			gr:hasValue "100" ;
			gr:hasUnitOfMeasurement "P1" ] .

:var_CloudServer_Availability_MeasurementPeriod a sla:Variable ;
	rdfs:label "Availability measurment period"@en ;
	sla:hasDefault 
		[ a genVoc:MeasurementPeriod ;
			gr:hasValue "1" ;
			gr:hasUnitOfMeasurement "MON" ] .

:var_CloudServer_Availability_Network a sla:Variable ;
	rdfs:label "Definition unavailability: Network"@en ;
	dcterms:description "The Network is considered 'not available' if data cannot be transmitted and received across the Network in the normal course of operation. By 'Network' we mean the portion of the ReliaCloud Server system extending from the network egress point of your Host Server (as defined below) to the outbound port of the ReliaCloud data center border router."@en .

:var_CloudServer_Availability_Infrastructure a sla:Variable ;
	rdfs:label "Definition unavailability: Network"@en ;
	dcterms:description "The Infrastructure is considered 'not available' if the Infrastructure is not functioning at a reasonable capacity as a result of power or cabling. By 'Infrastructure' we mean ReliaCloud's power (including UPS equipment, generator and cabling) systems and HVAC for the data center."@en .

:var_CloudServer_Availability_HostServer a sla:Variable ;
	rdfs:label "Definition unavailability: Network"@en ;
	dcterms:description "The Host Servers are considered 'not available' if they fail to function properly as a result of hardware and hypervisor layer problems on the Host Servers. By 'Host Server' we mean the hardware, computing and storage nodes, and software hypervisor of the physical servers which are used to host your Cloud Server(s). By 'Cloud Server' we mean specific and unique virtual machine instances which you have purchased through the ReliaCloud Server Product as specified in your particular Order."@en .

#____________________________________________________________________________________
:slo_CloudServer_Availability_Exclusions a sla:GuaranteedState ;
	dcterms:title "Availability exclusions"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Notwithstanding any provision to the contrary in this SLA, the following do not constitute Incidents:"@en ;
			sla:hasVariable 
				:var_CloudServer_Availability_Exclusion_ScheduledMaintenance ,
				:var_CloudServer_Availability_Exclusion_Negligence ,
				:var_CloudServer_Availability_Exclusion_Hackers ,
				:var_CloudServer_Availability_Exclusion_DNS ,
				:var_CloudServer_Availability_Exclusion_InternetAnomalies ,
				:var_CloudServer_Availability_Exclusion_DesasterEvents ,
				:var_CloudServer_Availability_Exclusion_InvalidReportOrRequest ,
				:var_CloudServer_Availability_Exclusion_BetaTests ] .

:var_CloudServer_Availability_Exclusion_ScheduledMaintenance a sla:Variable ;
	rdfs:label "Scheduled Maintenance"@en ;
	dcterms:description "Unavailability occurring during scheduled maintenance periods. 'Maintenance' means: (i) maintenance on ReliaCloud equipment that ReliaCloud has planned in advance; or (ii) any emergency maintenance, corrective action intended to remedy conditions likely to cause severe Service degradation, as designated by ReliaCloud in its sole discretion."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_Negligence a sla:Variable ;
	rdfs:label "Negligence"@en ;
	dcterms:description "Unavailability caused by negligent acts or omissions of you or your representatives, or by any of your applications, equipment, or facilities."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_Hackers a sla:Variable ;
	rdfs:label "Hackers and similar"@en ;
	dcterms:description "Outages caused by hackers, sabotage, viruses, worms, or other third party wrongful actions."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_DNS a sla:Variable ;
	rdfs:label "DNS issues"@en ;
	dcterms:description "DNS issues outside of ReliaCloud's control."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_InternetAnomalies a sla:Variable ;
	rdfs:label "Internet anomalies"@en ;
	dcterms:description "Outages resulting from Internet anomalies outside of ReliaCloud's control"@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_DesasterEvents a sla:Variable ;
	rdfs:label "Desaster events"@en ;
	dcterms:description "Outages resulting from fires, explosions, or force majeure"@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_InvalidReportOrRequest a sla:Variable ;
	rdfs:label "Invalid report or request"@en ;
	dcterms:description "Your failure to abide by the requirements of reporting an incident or requesting a credit as specified in the SLA."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_CloudServer_Availability_Exclusion_BetaTests a sla:Variable ;
	rdfs:label "Beta tests"@en ;
	dcterms:description "Unavailability related to a Service under 'beta test'."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

#____________________________________________________________________________________
:slo_CloudServer_UnavailabilityReport a sla:GuaranteedState ;
	dcterms:title "Report of unavailability"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "If you become aware that any of your Services are not available, you must promptly give ReliaCloud notice of the problem. You must reasonably cooperate with the ReliaCloud support staff by providing reasonable and timely responses to questions and other information needs the support staff may have in resolving the problem."@en ;
			sla:hasVariable 
				:var_CloudServer_UnavailabilityReport_Contact ,
				:var_CloudServer_UnavailabilityReport_Content_AccUsername ,
				:var_CloudServer_UnavailabilityReport_Content_InstanceID ,
				:var_CloudServer_UnavailabilityReport_Content_DateTime ,
				:var_CloudServer_UnavailabilityReport_Content_Estimates ,
				:var_CloudServer_UnavailabilityReport_CreditEligibility ] .

:var_CloudServer_UnavailabilityReport_Contact a sla:Variable ;
	rdfs:label "Request contact"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Send your report via email to sla@reliacloud.com."@en ] .
 
:var_CloudServer_UnavailabilityReport_Content_AccUsername a sla:Variable ;
	rdfs:label "Report content: Username"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your report must contain your ReliaCloud account username."@en ] .

:var_CloudServer_UnavailabilityReport_Content_InstanceID a sla:Variable ;
	rdfs:label "Report content: Instance ID"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your report must contain the ID of the cloud instance affected."@en ] .

:var_CloudServer_UnavailabilityReport_Content_DateTime a sla:Variable ;
	rdfs:label "Report content: Date and Time"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your report must contain the date(s) and time(s) of the Incident(s)."@en ] .

:var_CloudServer_UnavailabilityReport_CreditEligibility a sla:Variable ;
	rdfs:label "Credit Eligibility"@en ;
	dcterms:description "To be eligible for credits, the Incident must have been reported to ReliaCloud as described above, either while happening or promptly (within one hour) after the close of the Incident."@en ;
	sla:hasDefault 
		[ a genVoc:AbsoluteTimePeriod ;
			gr:hasMaxValue "1" ;
			gr:hasUnitOfMeasurement "HUR" ] .

#____________________________________________________________________________________
:slo_CloudServer_Credit_Calculation a sla:GuaranteedState ;
	dcterms:title "Credit calculation"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "A period of non-availability will be deemed to have started at the time we log when you first contact us about the problem. The end of the period of non-availability will be determined by ReliaCloud support staff based on ReliaCloud log files. Any period of time ReliaCloud does not meet the SLA availability requirements described above will be considered an 'Incident'. Incidents will be measured by the minute."@en ;
			sla:hasVariable 
				:var_CloudServer_Credit_Calculation_Start ,
				:var_CloudServer_Credit_Calculation_Slice ,
				:var_CloudServer_Credit_Calculation_End ] .

:var_CloudServer_Credit_Calculation_Start a sla:Variable ;
	rdfs:label "Credit period start on report"@en ;
	sla:hasDefault 
		[ a cloud:CreditPeriodStart ;
			gr:valueReference [ a cloud:ReportOfDowntime ] ] .

:var_CloudServer_Credit_Calculation_Slice a sla:Variable ;
	rdfs:label "Credit period is measured per minute"@en ;
	sla:hasDefault 
		[ a cloud:CreditPeriodSlice ;
			gr:hasValue "1" ;
			gr:hasUnitOfMeasurement "MIN" ] .

:var_CloudServer_Credit_Calculation_End a sla:Variable ;
	rdfs:label "Credit period end is dertermined by ReliaCloud support staff"@en ;
	sla:hasDefault 
		[ a cloud:CreditPeriodEnd ;
			gr:valueReference [ a :CloudServer_EndOfDowntime] ] .

#____________________________________________________________________________________
:slo_CloudServer_Credit_Suspension a sla:GuaranteedState ;
	dcterms:title "Credit suspension"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Reasons for the suspension of credit entitlement."@en ;
			sla:hasVariable 
				:var_CloudServer_Credit_Suspension_BreachTermsOfService ,
				:var_CloudServer_Credit_Suspension_BreachAcceptableUsePolicy ] .

:var_CloudServer_Credit_Suspension_BreachTermsOfService a sla:Variable ;
	rdfs:label "Breach of the Terms of Service during the period of an Incident."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

:var_CloudServer_Credit_Suspension_BreachAcceptableUsePolicy a sla:Variable ;
	rdfs:label "Breach of the Acceptable Use Policy during the period of an Incident."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

#____________________________________________________________________________________
:slo_CloudServer_Credit_Request a sla:GuaranteedAction ;
	dcterms:title "Credit request"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "To obtain a credit for a particular Incident, you must submit a request for credit within five business days after the conclusion of the Incident."@en ;
			sla:hasVariable 
				:var_CloudServer_Credit_Request_Period ,
				:var_CloudServer_Credit_Request_Contact ,
				:var_CloudServer_Credit_Request_Content_AccUsername ,
				:var_CloudServer_Credit_Request_Content_InstanceID ,
				:var_CloudServer_Credit_Request_Content_DateTime ,
				:var_CloudServer_Credit_Request_Content_Estimates ] .

:var_CloudServer_Credit_Request_Period a sla:Variable ;
	rdfs:label "Request period"@en ;
	sla:hasDefault 
		[ a cloud:CreditRequestPeriod ;
			gr:hasValue "30" ;
			gr:hasUnitOfMeasurement "E49" ] . # Working day

:var_CloudServer_Credit_Request_Contact a sla:Variable ;
	rdfs:label "Request contact"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Send your credit request via email to sla@reliacloud.com."@en ] .
 
:var_CloudServer_Credit_Request_Content_AccUsername a sla:Variable ;
	rdfs:label "Request content: Username"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your request must contain your ReliaCloud account username."@en ] .

:var_CloudServer_Credit_Request_Content_InstanceID a sla:Variable ;
	rdfs:label "Request content: Instance ID"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your request must contain the ID of the cloud instance affected."@en ] .

:var_CloudServer_Credit_Request_Content_DateTime a sla:Variable ;
	rdfs:label "Request content: Date and Time"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your request must contain the date(s) and time(s) of the Incident(s)."@en ] .

:var_CloudServer_Credit_Request_Content_Estimates a sla:Variable ;
	rdfs:label "Request content: Request and estimates"@en ;
	sla:hasDefault 
		[ a gr:QualitativeValue ;
			rdfs:label "Your request must contain  a request for a credit under this SLA and any estimates you have for the credit amount."@en ] .

