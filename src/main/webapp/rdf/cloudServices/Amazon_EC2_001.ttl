#************************************************************************************
#	SLAs EC2															
#************************************************************************************

:slp_EC2 a usdl:ServiceLevelProfile ;
	dcterms:title "Amazon Web Services EC2 Service Level Profile"@en ;
	dcterms:description "The Amazon Web Services EC2 Service Level Profile is accessible via 'http://aws.amazon.com/ec2-sla/'. All information integrated here are not legally binding." ;
	sla:hasServiceLevel 
		:slo_EC2_AvailabilityGuarantee , 
		:slo_EC2_AvailibilityCalculation , 
		:slo_EC2_AvailabilityExclusions , 
		:slo_EC2_CreditSuspension , 
		:slo_EC2_CreditRequest .

#____________________________________________________________________________________
:slo_EC2_AvailabilityGuarantee a sla:GuaranteedState ;
	dcterms:title "Annual Uptime Percentage"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "AWS will use commercially reasonable efforts to make Amazon EC2 available with an 'Annual Uptime Percentage' of at least 99.95% during the 'Service Year'."@en ;
			sla:hasVariable 
				:var_EC2_AvailabilityGuarantee_Value , 
				:var_EC2_AvailabilityGuarantee_MeasurementPeriod ] .

:var_EC2_AvailabilityGuarantee_Value a sla:Variable ;
	rdfs:label "Annual Uptime Percentage"@en ;
	sla:hasDefault 
		[ a cloud:Availability ;
			gr:hasValue "99.95" ;
			gr:hasUnitOfMeasurement "P1" ] .

:var_EC2_AvailabilityGuarantee_MeasurementPeriod a sla:Variable ;
	rdfs:label "Annual Uptime Percentage measurment period"@en ;
	sla:hasDefault 
		[ a genVoc:MeasurementPeriod ;
			gr:hasValue "365" ;
			gr:hasUnitOfMeasurement "DAY" ] .

#____________________________________________________________________________________
:slo_EC2_AvailibilityCalculation a sla:GuaranteedState ;
	dcterms:title "Annual Uptime Percentage calculation"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "'Annual Uptime Percentage' is calculated by subtracting from 100% the percentage of 5 minute periods during the 'Service Year' in which Amazon EC2 was in the state of 'Region Unavailable'."@en;
			sla:hasVariable 
				:var_EC2_AvailibilityCalculation_CreditPeriodStart ,
				:var_EC2_AvailibilityCalculation_CreditPeriodSlice ,
				:var_EC2_AvailibilityCalculation_CreditPeriodEnd ] .

:var_EC2_AvailibilityCalculation_CreditPeriodStart a sla:Variable ;
	rdfs:label "Credit period start"@en ;
	sla:hasDefault 
		[ a cloud:CreditPeriodStart ;
			gr:valueReference 
				[ a :Amazon_EC2_CreditPeriodStart ] ] .

:var_EC2_AvailibilityCalculation_CreditPeriodSlice a sla:Variable ;
	rdfs:label "Credit period slice"@en ;
	sla:hasDefault 
		[ a cloud:CreditPeriodSlice ;
			gr:hasValue "5" ;
			gr:hasUnitOfMeasurement "MIN" ] .

:var_EC2_AvailibilityCalculation_CreditPeriodEnd a sla:Variable ;
	rdfs:label "Credit period end"@en ;
	sla:hasDefault 
		[ a cloud:CreditPeriodEnd ;
			gr:valueReference 
				[ a :Amazon_EC2_CreditPeriodEnd ] ] .

#____________________________________________________________________________________
:slo_EC2_AvailabilityExclusions a sla:GuaranteedState ;
	dcterms:title "Exceptions to Annual Uptime Percentage calculation" ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Timespans not considered in calculating Annual Uptime Percentage."@en ;
			sla:hasVariable 
				:var_EC2_AvailabilityExclusions_Suspension , 
				:var_EC2_AvailabilityExclusions_OutsideControl , 
				:var_EC2_AvailabilityExclusions_ActionsAndInactions ,
				:var_EC2_AvailibilityExclusions_EquipmentNotInDirectControl ,
				:var_EC2_AvailibilityExclusions_FailuresOfIndividualInstances ] .

:var_EC2_AvailabilityExclusions_Suspension a sla:Variable ;
	rdfs:label "Downtime resulting from suspensions."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_EC2_AvailabilityExclusions_OutsideControl a sla:Variable ;
	rdfs:label "Downtime caused by factors outside AWS's reasonable control, including force majeure or Internet access or related problems beyond the demarcation point of Amazon EC2."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_EC2_AvailabilityExclusions_ActionsAndInactions a sla:Variable ;
	rdfs:label "Downtime resulting from any actions or inactions of the customer or any third party."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_EC2_AvailabilityExclusions_EquipmentNotInDirectControl a sla:Variable ;
	rdfs:label "Downtime resulting from the customer's equipment, software or other technology and/or third party equipment, software or other technology (other than third party equipment within AWS's direct control). "@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

:var_EC2_AvailabilityExclusions_FailuresOfIndividualInstances a sla:Variable ;
	rdfs:label "Downtime resulting from failures of individual instances not attributable to Region Unavailability."@en ;
	sla:hasDefault [ a cloud:AvailabilityExclusion ] .

#____________________________________________________________________________________
:slo_EC2_CreditSuspension a sla:GuaranteedState ;
	dcterms:title "Full credit suspension"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Full suspension of credit eligibility in the following cases."@en ;
			sla:hasVariable 
				:var_EC2_CreditSuspension_RequestNotInTime ,
				:var_EC2_CreditSuspension_SuspensionOrTermination ] .

:var_EC2_CreditSuspension_RequestNotInTime a sla:Variable ;
	rdfs:label "The customer did not send the request in time."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

:var_EC2_CreditSuspension_SuspensionOrTermination a sla:Variable ;
	rdfs:label "The customer's right to use EC2 was suspended orinated."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

#____________________________________________________________________________________
:slo_EC2_CreditRequest a sla:GuaranteedAction ;
	dcterms:title "Credit request"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "To receive a credit, the customer must submit a request by sending an e-mail message to aws-sla-request@amazon.com including the necessary information within 30 business days"@en ;
			sla:hasVariable 
				:var_EC2_CreditRequest_AccountNumber ,
				:var_EC2_CreditRequest_DatesAndTimes ,
				:var_EC2_CreditRequest_Logs ,
				:var_EC2_CreditRequest_CreditRequestPeriod ] .

:var_EC2_CreditRequest_AccountNumber a sla:Variable ;
	rdfs:label "The subject of the e-mail must contain the customer's account number."@en .

:var_EC2_CreditRequest_DatesAndTimes a sla:Variable ;
	rdfs:label "The body of the e-mail must contain the dates and times of each incident."@en .

:var_EC2_CreditRequest_Logs a sla:Variable ;
	rdfs:label "The e-mail must contain the customer's server request logs thatdocument the errors."@en .

:var_EC2_CreditRequest_CreditRequestPeriod a sla:Variable ;
	rdfs:label "Request period"@en ;
	sla:hasDefault 
		[ a cloud:CreditRequestPeriod ;
			gr:hasValue "30" ;
			gr:hasUnitOfMeasurement "E49" ] . # Common Code for Working day

