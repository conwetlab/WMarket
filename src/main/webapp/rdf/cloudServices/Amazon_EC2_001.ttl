			gr:hasValue "7" ;
			gr:hasUnitOfMeasurement "P1" ] ;
	price:hasFence
		[ a price:PriceFence ;
			price:fenceExpression "10000=<x<=80000" ;
			price:fenceUnit "USD" ] .

:priceComponent_Support_Gold_Gold_80Kplus a price:PriceComponent ;
	dcterms:title "Relative fee"@en ;
	dcterms:description "5% of monthly AWS usage from $80K+"@en ;
	price:hasPrice 
		[ a gr:QuantitativeValue ;
			gr:hasValue "5" ;
			gr:hasUnitOfMeasurement "P1" ] ;
	price:hasFence
		[ a price:PriceFence ;
			price:fenceExpression "x<=80000" ;
			price:fenceUnit "USD" ] .

