		<usdl:hasServiceLevelProfile>
			<usdl:ServiceLevelProfile rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#slp_CloudHosting">
				<dcterms:title xml:lang="en">Service Level Profile of Elastic Hosts Cloud Hosting service</dcterms:title>
				<dcterms:description xml:lang="en">All information integrated here are not legally binding. Please consult the SLA section in the terms-of-service under 'http://www.elastichosts.com/cloud-hosting/terms-of-service' for a detailed description.</dcterms:description>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#slo_CloudHosting_AvailabilityGuarantee">
						<dcterms:title xml:lang="en">Availibility</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">We guarantee that your stored data and running virtual servers will be available over the internet for 100% of the time in any given calendar month</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_AvailabilityGuarantee_Value">
										<rdfs:label xml:lang="en">Percentage Value</rdfs:label>
										<sla:hasDefault>
											<cloud:Availability>
												<gr:hasValue>100</gr:hasValue>
												<gr:hasUnitOfMeasurement>P1</gr:hasUnitOfMeasurement>
											</cloud:Availability>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_AvailabilityGuarantee_MeasurementPeriod">
										<rdfs:label xml:lang="en">Measurment period</rdfs:label>
										<sla:hasDefault>
											<genVoc:MeasurementPeriod>
												<gr:hasValue>1</gr:hasValue>
												<gr:hasUnitOfMeasurement>MON</gr:hasUnitOfMeasurement>
											</genVoc:MeasurementPeriod>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel rdf:resource="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#slo_CloudHosting_AvailibilityExclusions" />
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#slo_CloudHosting_AvailibilityCalculation">
						<dcterms:title>Availibility calculation</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">If we fail to meet the guarantee availibility, then you will be able to request a credit up to a maximum of 100% of your fee for capacity used during the previous 30 calendar days. You are eligible for a credit of 100 times the fees for any period of unavailability lasting more than 15 minutes, measured from the time at which you validly inform us at support@elastichosts.com of the unavailability or the time at which our monitoring systems detect the unavailability, whichever is earlier. In the event that we fail to meet the guarantee on more than one occasion within a period of 30 calendar days, then the credit that you may claim for any incident will be limited to the maximum of 100% of your fee for capacity used since the previous incident or 100% of your fee for capacity used during the previous 30 calendar days, whichever fee is lower.</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_AvailibilityCalculation_CreditPeriodStart">
										<rdfs:label xml:lang="en">Credit period start</rdfs:label>
										<sla:hasDefault>
											<cloud:CreditPeriodStart>
												<gr:valueReference>
													<genVoc:GenericPointInTime>
														<skos:prefLabel>Information via 'support@elastichosts.com'.</skos:prefLabel>
														<skos:broader rdf:resource="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/vocabulary/Vocabulary_Cloud_004#CreditPeriodStart" />
													</genVoc:GenericPointInTime>
												</gr:valueReference>
												<gr:valueReference>
													<genVoc:GenericPointInTime>
														<skos:prefLabel>Detection via monitoring system.</skos:prefLabel>
														<skos:broader rdf:resource="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/vocabulary/Vocabulary_Cloud_004#CreditPeriodStart" />
													</genVoc:GenericPointInTime>
												</gr:valueReference>
											</cloud:CreditPeriodStart>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_AvailibilityCalculation_CreditPeriodMin">
										<rdfs:label xml:lang="en">Minimal credit period</rdfs:label>
										<sla:hasDefault>
											<cloud:CreditPeriodMinimum>
												<gr:hasValue>15</gr:hasValue>
												<gr:hasUnitOfMeasurement>MIN</gr:hasUnitOfMeasurement>
											</cloud:CreditPeriodMinimum>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#slo_CloudHosting_DataLoss">
						<dcterms:title>Permanent data loss</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">In the case of permanent loss of your stored data, you are eligible for a credit of your entire fee for the previous 30 calendar days.</dcterms:description>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedAction rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#slo_CloudHosting_CreditRequest">
						<dcterms:title xml:lang="en">Credit request</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">To receive a credit, you must submit a request by sending an e-mail message to support@elastichosts.com including the necessary information within 30 calender days</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_CreditRequest_StartTime">
										<rdfs:label xml:lang="en">The email must contain the start time of the period of unavailability.</rdfs:label>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_CreditRequest_Date">
										<rdfs:label xml:lang="en">The email must contain the date of the period of unavailability.</rdfs:label>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_CreditRequest_Duration">
										<rdfs:label xml:lang="en">The email must contain the duration of the period of unavailability.</rdfs:label>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/ElasticHosts_CloudHosting_001#var_CloudHosting_CreditRequest_CreditRequestPeriod">
										<rdfs:label xml:lang="en">Request period</rdfs:label>
