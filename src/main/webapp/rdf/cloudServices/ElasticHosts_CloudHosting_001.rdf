						<sla:hasDefault>
							<cloud:AvailabilityExclusion />
						</sla:hasDefault>
					</sla:Variable>
				</sla:hasVariable>
			</sla:ServiceLevelExpression>
		</sla:serviceLevelExpression>
	</sla:GuaranteedState>
