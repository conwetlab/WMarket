								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_Availability_Network_Exclusion_ScheduledMaintenance">
										<rdfs:label xml:lang="en">Scheduled Maintenance</rdfs:label>
										<sla:hasDefault>
											<RackspaceCloudServers:Rackspace_NetworkAvailability_Exclusion />
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_Availability_DataCentreInfrastructure">
						<dcterms:title xml:lang="en">Availability: Data centre infrastructure</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">We guaranty that data center HVAC and power will be functioning 100% of the time in any given monthly billing period, excluding scheduled maintenance. Infrastructure downtime exists when Cloud Servers downtime occurs as a result of power or heat problems.</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_Availability_DataCentreInfrastructure_Value">
										<rdfs:label xml:lang="en">Data centre infrastructure availability value</rdfs:label>
										<sla:hasDefault>
											<RackspaceCloudServers:Rackspace_DataCenterInfrastructureAvailability>
												<gr:hasValue>100</gr:hasValue>
												<gr:hasUnitOfMeasurement>P1</gr:hasUnitOfMeasurement>
											</RackspaceCloudServers:Rackspace_DataCenterInfrastructureAvailability>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_Availability_DataCentreInfrastructure_Exclusion">
						<dcterms:title xml:lang="en">Availability exclusion: Data centre infrastructure</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">Data center infrastructure availability is not influenced by scheduled maintenance.</dcterms:description>
								<sla:hasVariable rdf:resource="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_Availability_Network_Exclusion_ScheduledMaintenance" />
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_CloudServerHostsRTO">
						<dcterms:title xml:lang="en">Cloud Server Hosts RTO</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">We guaranty the functioning of all cloud server hosts including compute, storage, and hypervisor. If a cloud server host fails, we guaranty that restoration or repair will be complete within one hour of problem identification.</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_CloudServerHostsRTO_Value">
										<rdfs:label xml:lang="en">Cloud Server Hosts recovery time objective (RTO)</rdfs:label>
										<sla:hasDefault>
											<cloud:RecoveryTimeObjective>
												<gr:hasValue>1</gr:hasValue>
												<gr:hasUnitOfMeasurement>HUR</gr:hasUnitOfMeasurement>
											</cloud:RecoveryTimeObjective>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_Migration">
						<dcterms:title xml:lang="en">Migration</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">If a cloud server migration is required because of cloud server host degradation, we will notify you at least 24 hours in advance of beginning the migration, unless we determine in our reasonable judgment, that we must begin the migration sooner to protect your cloud server data. Either way, we guaranty that the migration will be complete within three hours of the time that we begin the migration.</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_Migration_Notification">
										<rdfs:label xml:lang="en">Notification at least 24h in advance</rdfs:label>
										<sla:hasDefault>
											<genVoc:AbsoluteTimePeriod>
												<gr:hasMinValue>24</gr:hasMinValue>
												<gr:hasUnitOfMeasurement>HUR</gr:hasUnitOfMeasurement>
											</genVoc:AbsoluteTimePeriod>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_Migration_Duration">
										<rdfs:label xml:lang="en">Duration of migration max 3h</rdfs:label>
										<sla:hasDefault>
											<genVoc:AbsoluteTimePeriod>
												<gr:hasMaxValue>3</gr:hasMaxValue>
												<gr:hasUnitOfMeasurement>HUR</gr:hasUnitOfMeasurement>
											</genVoc:AbsoluteTimePeriod>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_AvailibilityCalculation_Network">
						<dcterms:title xml:lang="en">Crediting: Network</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">If we fail to meet a guaranty for network availability, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle).</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_AvailibilityCalculation_Network_Slice">
										<rdfs:label xml:lang="en">Credit equal to 5% credit of the fees for each 30 minutes of downtime, up to 100%.</rdfs:label>
										<sla:hasDefault>
											<RackspaceCloudServers:Rackspace_CreditPeriodSlice_Network>
												<gr:hasValue>30</gr:hasValue>
												<gr:hasUnitOfMeasurement>MIN</gr:hasUnitOfMeasurement>
											</RackspaceCloudServers:Rackspace_CreditPeriodSlice_Network>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_AvailibilityCalculation_DataCenterInfrastructure">
						<dcterms:title xml:lang="en">Crediting: Data center infrastructure</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">If we fail to meet a guaranty for infrastructure availability, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle).</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_AvailibilityCalculation_DataCenterInfrastructure_Slice">
										<rdfs:label xml:lang="en">Credit equal to 5% credit of the fees for each 30 minutes of downtime, up to 100%.</rdfs:label>
										<sla:hasDefault>
											<RackspaceCloudServers:Rackspace_CreditPeriodSlice_DataCenterInfrastructure>
												<gr:hasValue>30</gr:hasValue>
												<gr:hasUnitOfMeasurement>MIN</gr:hasUnitOfMeasurement>
											</RackspaceCloudServers:Rackspace_CreditPeriodSlice_DataCenterInfrastructure>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_AvailibilityCalculation_CloudServerHosts">
						<dcterms:title xml:lang="en">Crediting: Cloud Server Hosts RTO</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">If we fail to meet a guaranty for Cloud Server Hosts RTO, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle).</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_AvailibilityCalculation_CloudServerHosts_Value">
										<rdfs:label xml:lang="en">Credit equal to 5% of the fees for each additional hour of downtime, up to 100%.</rdfs:label>
										<sla:hasDefault>
											<RackspaceCloudServers:Rackspace_CreditPeriodSlice_CloudServerHosts>
												<gr:hasValue>1</gr:hasValue>
												<gr:hasUnitOfMeasurement>HUR</gr:hasUnitOfMeasurement>
											</RackspaceCloudServers:Rackspace_CreditPeriodSlice_CloudServerHosts>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_AvailibilityCalculation_Migration">
						<dcterms:title xml:lang="en">Crediting: Migration</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">If we fail to meet a guaranty for duration of migration, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle).</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_AvailibilityCalculation_Migration_Value">
										<rdfs:label xml:lang="en">Credit equal to 5% of the fees for each additional hour of downtime, up to 100%.</rdfs:label>
										<sla:hasDefault>
											<RackspaceCloudServers:Rackspace_CreditPeriodSlice_Migration>
												<gr:hasValue>1</gr:hasValue>
												<gr:hasUnitOfMeasurement>HUR</gr:hasUnitOfMeasurement>
											</RackspaceCloudServers:Rackspace_CreditPeriodSlice_Migration>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedState rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_CreditSuspension">
						<dcterms:title xml:lang="en">Credit suspension</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">Reasons for the suspension of credit entitlement.</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_CreditSuspension_SLABreach">
										<rdfs:label xml:lang="en">You are not entitled to a credit if you are in breach of your services agreement with The Rackspace Cloud (including your payment obligations to us) until you have cured the breach.</rdfs:label>
										<sla:hasDefault>
											<cloud:CreditSuspensionReason />
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_CreditSuspension_AgreementBreach">
										<rdfs:label xml:lang="en">You are not entitled to a credit if downtime would not have occurred but for your breach of your agreement with The Rackspace Cloud.</rdfs:label>
										<sla:hasDefault>
											<cloud:CreditSuspensionReason />
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_CreditSuspension_Misuese">
										<rdfs:label xml:lang="en">You are not entitled to a credit if downtime would not have occurred but for your misuse of Cloud Servers system.</rdfs:label>
										<sla:hasDefault>
											<cloud:CreditSuspensionReason />
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedState>
				</sla:hasServiceLevel>
				<sla:hasServiceLevel>
					<sla:GuaranteedAction rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#slo_Rackspace_CreditRequest">
						<dcterms:title xml:lang="en">Credit request</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
								<dcterms:description xml:lang="en">To receive a credit, you must contact your The Rackspace Cloud account team within thirty (30) days following the end of the downtime. You must show that your use of the Cloud Servers was adversely affected in some way as a result of the downtime to be eligible for the credit.</dcterms:description>
								<sla:hasVariable>
									<sla:Variable rdf:about="http://appsnserv.lab.fi-ware.eu/cloudservices/rdf/cloudServices/Rackspace_CloudServers_001#var_Rackspace_CreditRequest_CreditRequestPeriod">
										<rdfs:label xml:lang="en">Request period</rdfs:label>
										<sla:hasDefault>
											<cloud:CreditRequestPeriod>
												<gr:hasValue>30</gr:hasValue>
												<gr:hasUnitOfMeasurement>DAY</gr:hasUnitOfMeasurement>
											</cloud:CreditRequestPeriod>
										</sla:hasDefault>
									</sla:Variable>
								</sla:hasVariable>
							</sla:ServiceLevelExpression>
						</sla:serviceLevelExpression>
					</sla:GuaranteedAction>
				</sla:hasServiceLevel>
			</usdl:ServiceLevelProfile>
		</usdl:hasServiceLevelProfile>
	</usdl:ServiceOffering>
